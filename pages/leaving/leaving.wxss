.tabs {
    display: flex;
    padding: 50rpx 30rpx 20rpx;
}
.tabs_item {
    font-size: 48rpx;
    color: #222222;
    line-height: 58rpx;
}
.tabs_button {
    width:192rpx;
    height:58rpx;
    margin-left: 30rpx;
    background:rgba(74,115,230,1);
    border-radius:29rpx;
    color: #ffffff;
    text-align: center;
    line-height: 58rpx;
    font-size: 24rpx;
}

.title {
    padding: 0 30rpx 10rpx;
    color: #888888;
    font-size: 32rpx;
    line-height: 40rpx;
}
.content {
    width:690rpx;
    margin: 30rpx auto 0;
    box-sizing: border-box;
    background:rgba(255,255,255,1);
    box-shadow:0rpx 10rpx 20rpx 0rpx rgba(0,0,0,0.08);
    border-radius:20rpx;
}
.content {
    /* margin-bottom: 30rpx; */
    padding: 0 30rpx;
}
.list_item {
    padding: 50rpx 0 30rpx;
    display: flex;
    display: -webkit-flex;
    justify-content: space-between;
    -webkit-justify-content: space-between;
    font-size: 32rpx;
    line-height: 40rpx;
    border-bottom: 2rpx solid rgba(238,238,238,1);
}
.list_item:last-child{
    border-bottom: 0;
}
.item_label {
    flex: 1;
    -webkit-flex: 1;
    color: #222222;
}
.item_value,.item_value_width {
    color: #888888;
}
.item_address {
    padding-top: 30rpx;
    color: #888888;
}
.submit_btn,.submit_btn_disabled {
    width:690rpx;
    height:100rpx;
    margin: 40rpx auto 150rpx;
    box-shadow:0rpx 10rpx 20rpx 0rpx rgba(74,115,230,0.3);
    border-radius:50rpx;
    color: #ffffff;
    line-height: 100rpx;
    font-size: 36rpx;
    font-weight: 700;
}
.submit_btn{
    background:rgba(74,115,230,1);
}
.submit_btn_disabled {
    background:rgba(74,115,230,.7);
}
.number_input {
    width: 450rpx;
    font-size: 32rpx;
    line-height: 40rpx;
    color: #222222;
    text-align: right;
}
.remarks_txt {
    margin-top: 30rpx;
    line-height: 40rpx;
    font-size: 32rpx;
}
.placeholder {
    color: #CCCCCC;
}
.add_button {
    width:690rpx;
    height:100rpx;
    margin: 40rpx auto;
    background:rgba(255,255,255,1);
    box-shadow:0rpx 10rpx 20rpx 0rpx rgba(0,0,0,0.08);
    border-radius:60rpx;
    color: #4A73E6;
    line-height: 100rpx;
    font-size: 36rpx;
}
.width_flex {
    width: 400rpx;
}
.width_430 {
    width: 430rpx;
}
.add_button_style {
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
}
.add_button_style::before{
    width:24rpx;
    height:24rpx;
    margin-right: 20rpx;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: 100% auto;
    content: "";
}
.pos {
    position: relative;
}
.tartget_lists {
    width: 690rpx;
    padding-left: 200rpx;
    box-sizing: border-box;
    height: 300rpx;
    overflow: auto;
    position: absolute;
    top: 125rpx;
    right:-26rpx;
    background: #ffffff;
    z-index: 9;
}
.clear {
    position: absolute;
    top: -10rpx;
    right: -10rpx;
}
.item_value_width {
    width: 430rpx;
}
.picker_txt_select {
    color: #222222;
}
.picker_txt {
    color: #4A73E6;
    font-size: 32rpx;
    line-height: 40rpx;
}
.win_dialog {
    width: 100%;
    height: 100%;
    padding-top: 30%;
    background: rgba(0,0,0,0.5);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 999;
    display: flex;
    /* align-items: center; */
    justify-content: center;
    box-sizing: border-box;
}
.win_dialog_focus {
    width: 100%;
    height: 100%;
    padding-top: 20%;
    background: rgba(0,0,0,0.5);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 999;
    display: flex;
    /* align-items: center; */
    justify-content: center;
    box-sizing: border-box;
}
.win_dialog_con {
    width:690rpx;
    /* height: 420rpx; */
    /* margin: 0 auto 40rpx; */
    box-sizing: border-box;
    padding: 0 30rpx 30rpx;
    background:rgba(255,255,255,1);
    box-shadow:0rpx 10rpx 20rpx 0rpx rgba(0,0,0,0.08);
    border-radius:20rpx;
}
.dialog_input,.dialog_input_small,.dialog_input_sub {
    font-size: 32rpx;
    line-height: 40rpx;
    color: #222222;
    text-align: left;
}
.dialog_input_tab {
    width: 450rpx;
    font-size: 32rpx;
    line-height: 42rpx;
    color: #222222;
    text-align: left;
}
.dialog_input {
    width: 500rpx;
}
.dialog_input_small {
    width: 60rpx;
    margin: 0 5rpx;
    border-bottom: #CCCCCC 1rpx solid;
    text-align: center;
}
.dialog_input_sub {
    width: 450rpx;
}
.dialog_input_small:last-child{
    width: 70rpx;
}
.item_width_con {
    width: 500rpx;
    display: flex;
}
.dialog_submit_btn,.dialog_submit_btn_tab {
    width:300rpx;
    margin: auto;
    height:80rpx;
    margin: 30rpx auto;
    box-shadow:0rpx 10rpx 20rpx 0rpx rgba(74,115,230,0.3);
    
    border-radius:50rpx;
    color: #ffffff;
    line-height: 80rpx;
    font-size: 36rpx;
    font-weight: 700;
}
.dialog_submit_btn {
    background:rgba(74,115,230,1);
}
.dialog_submit_btn_tab{
    background:rgba(74,115,230,.7);
}
.position {
    position: relative;
    background-color: #ffffff;
}
.position_absolute {
    width: 480rpx;
    max-height: 300rpx;
    min-height: 240rpx;
    border-bottom-left-radius: 20rpx;
    border-bottom-right-radius: 20rpx;
    overflow: auto;
    padding: 0 30rpx;
    box-sizing: border-box;
    position: absolute;
    right: 0;
    top: 0;
    background: #ffffff;
    z-index: 99;
}
.position_txt {
    padding: 20rpx 0;
    font-size: 28rpx;
    line-height: 40rpx;
    border-bottom: 1px solid rgba(0,0,0,0.08);
}
.position_txt_none {
    padding: 20rpx 0;
    font-size: 28rpx;
    line-height: 40rpx;
    border-bottom: 1px solid rgba(0,0,0,0.08);
    text-align: center;
}